{
  "name": "practice-webpack",
  "version": "1.0.0",
  "main": "src/app.js",
  "scripts": {
    "build": "webpack --config ./config/webpack.config.js",
    "build-command": "webpack ./src/app.js ./dist/app.js --target=node && node ./dist/app.js",
    "build-start": "npm run build && node ./dist/app.js",
    "webpack-watch": "webpack --config ./config/webpack.config.js --watch",
    "webpack-web-start": "webpack-dev-server --config ./config/webpack.config.js --open",
    "test": "cd ./dist node ./app.js"
  },
