    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "md5": "^2.2.1",
    "net": "^1.0.2",
    "request": "^2.83.0",
    "tls": "0.0.1"
  },
